{
  "manifest_version": 3,
  "name": "moto",
  "version": "1.0",
  "description": "nettimoto",
  "content_scripts": [
    {
      "js": [
        "scripts/parser.js"
      ],
      "matches": [
        "https://www.nettimoto.com/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["test.json"],
      "matches": ["<all_urls>"]
    }
  ]
  
}